<Window x:Class="WpfApp1.Reg"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="Reg" Height="250" Width="400">
    <Grid>
        <Button Content="назад" HorizontalAlignment="Left" Margin="23,10,0,0" VerticalAlignment="Top" Width="56" Click="Button_Click_1"/>
        <StackPanel Margin="10,50">
            <Label Content="Логин:"/>
            <TextBox x:Name="login"/>
            <Label Content="Пароль:"/>
            <TextBox x:Name="password"/>
            <Button Content="регестрация" Click="regButton_Click"/>
        </StackPanel>
    </Grid>
</Window>
